{
  "version": 2,
  "builds": [
    {
      "src": "webhook-server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/webhook",
      "dest": "/webhook-server.js"
    },
    {
      "src": "/health",
      "dest": "/webhook-server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "redirects": [
    {
      "source": "/admin",
      "destination": "/admin-login.html",
      "permanent": true
    },
    {
      "source": "/dashboard",
      "destination": "/admin-dashboard-new.html",
      "permanent": true
    }
  ]
}